<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com/XSD/maelstrom/userGroupOption.xsd">
	<import>
		<categories>
			<category name="user.jcoins">
				<parent>user</parent>
			</category>

			<category name="user.jcoins.general">
				<parent>user.jcoins</parent>
			</category>

			<category name="mod.jcoins">
				<parent>mod</parent>
			</category>

			<category name="mod.jcoins.general">
				<parent>mod.jcoins</parent>
			</category>

			<category name="admin.jcoins">
				<parent>admin</parent>
			</category>
			
			<category name="admin.jcoins.general">
				<parent>admin.jcoins</parent>
			</category>

			<category name="admin.jcoins.premiumgroups">
				<parent>admin.jcoins</parent>
			</category>
		</categories>

		<options>
			<option name="mod.jcoins.canModTransfer">
				<categoryname>mod.jcoins.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>

			<option name="mod.jcoins.canSeeTransferList">
				<categoryname>mod.jcoins.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>

			<option name="admin.jcoins.premiumgroups.canEditPremiumGroups">
				<categoryname>admin.jcoins.premiumgroups</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>


			<option name="admin.jcoins.canExecuteMassProcessing">
				<categoryname>admin.jcoins.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>

			<option name="user.jcoins.canTransfer">
				<categoryname>user.jcoins.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
		</options>
	</import>
</data>